{

  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "react/datagram": "^1.4",
    "react/socket": "^1.2",
    "smi2/phpclickhouse": "^1.3",
    "symfony/config": "^4.2",
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/dotenv": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/lock": "^4.4",
    "symfony/process": "^4.4",
    "symfony/yaml": "^4.2"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app"
      },
      "classmap": ["app"]
  },
  "require-dev": {
      "symfony/var-dumper": "^4.2"
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "7.2.0"
    },
    "sort-packages": true
  },
  "scripts": {
    "run": [
      "@php -f bin/udpipe server:start"
    ]
  }
}
